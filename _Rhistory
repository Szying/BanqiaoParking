UsedRate_x1908, UsedRate_x1912, UsedRate_x1916, UsedRate_x1920, UsedRate_x1923
) %>%
summarise(
"0800_AVRUR(%)" = round(mean(c(UsedRate_x1708,UsedRate_x1808,UsedRate_x1908)), digits = 3) * 100,
"1200_AVRUR(%)" = round(mean(c(UsedRate_x1712,UsedRate_x1812,UsedRate_x1912)), digits = 3) * 100,
"1600_AVRUR(%)" = round(mean(c(UsedRate_x1716,UsedRate_x1816,UsedRate_x1916)), digits = 3) * 100,
"2000_AVRUR(%)" = round(mean(c(UsedRate_x1720,UsedRate_x1820,UsedRate_x1920)), digits = 3) * 100,
"2300_AVRUR(%)" = round(mean(c(UsedRate_x1723,UsedRate_x1823,UsedRate_x1923)), digits = 3) * 100
)
